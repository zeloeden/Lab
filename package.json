{
  "name": "shadcnui",
  "type": "module",
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "vite --port 5173 --strictPort --open",
    "dev:with-bridge": "concurrently -n BRIDGE,WEB -c green,blue \"pnpm run bridge:start\" \"pnpm run dev:safe\"",
    "build": "vite build",
    "lint": "eslint --quiet ./src",
    "preview": "vite preview",
    "e2e": "start-server-and-test dev:safe http://localhost:5173 pnpm exec playwright test",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui",
    "bridge:start": "node scripts/bridge-runner.mjs",
    "scale:bridge": "node scripts/ja5003_ws_bridge.js",
    "scale:ensure": "node scripts/ensure-scale-bridge.mjs",
    "scale:install-bridge": "powershell -ExecutionPolicy Bypass -File scripts/install-scale-bridge.ps1",
    "scale:soft": "powershell -ExecutionPolicy Bypass -File scripts/scale-soft.ps1 -Com COM3 -Baud 9600 -Ms 250",
    "scale:bridge:soft": "cross-env JA_FORCE_OPEN=1 JA_PORT=COM3 JA_BAUD=9600 JA_CONTINUOUS=0 JA_POLL_CMDS=P JA_SI_MS=500 node scripts/ja5003_ws_bridge.js",
    "scale:bridge:cont": "cross-env JA_FORCE_OPEN=1 JA_PORT=COM3 JA_BAUD=9600 JA_CONTINUOUS=1 JA_POLL_CMDS=SI,S,P JA_SI_MS=1000 node scripts/ja5003_ws_bridge.js",
    "scale:bridge:auto": "cross-env JA_FORCE_OPEN=1 JA_PORT=AUTO JA_BAUD=9600 JA_CONTINUOUS=0 JA_POLL_CMDS=P JA_SI_MS=500 node scripts/ja5003_ws_bridge.js",
    "verify:mount": "node scripts/verify-mount.mjs",
    "dev:safe": "pnpm run verify:mount && vite --port 5173 --strictPort --open"
  },
